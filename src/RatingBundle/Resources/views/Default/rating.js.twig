function makeIFrame(url) {
    var ifrm = document.getElementById('rating_iframe');
    console.log(ifrm);
    if(!ifrm){
        ifrm = document.createElement("iframe");
        ifrm.id = 'rating_iframe';
        ifrm.setAttribute("src", url);
        ifrm.setAttribute("align", "center");
        ifrm.style.padding         = "0px";
        ifrm.style.margin          = "0px";
        ifrm.style.border          = "0px";
        ifrm.style.width           = "100px";
        ifrm.style.height          = "100px";
        ifrm.style.color           = '#fff';
        ifrm.style.position        = 'fixed';
        ifrm.style.bottom          = 0;
        ifrm.style.right           = 0;
        ifrm.style.zIndex          = 100;
        ifrm.style.fontFamily      = 'Arial, Helvetica, sans';
        document.body.appendChild(ifrm);
    }else{
        ifrm.setAttribute("src", url);
        ifrm.style.display="";
    }
    return false;
}
window.onload = function () {
    var url = "{{ url('rating_average', { 'userId': user.id }) | raw }}"
    makeIFrame(url)
}
